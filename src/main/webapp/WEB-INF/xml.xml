<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

     <servlet>
     	<servlet-name>UserInsertServlet</servlet-name>
     	<servlet-class>servlet.UserInsertServlet</servlet-class>
     </servlet>
     
     <servlet-mapping>
     	<servlet-name>UserInsertServlet</servlet-name>
     	<url-pattern>/UserInsertServlet</url-pattern>
     </servlet-mapping>
     
     <servlet>
     	<servlet-name>UserDisplayServlet</servlet-name>
     	<servlet-class>servlet.UserDisplayServlet</servlet-class>
     </servlet>
     
     <servlet-mapping>
     	<servlet-name>UserDisplayServlet</servlet-name>
     	<url-pattern>/UserDisplayServlet</url-pattern>
     </servlet-mapping>
     
     <servlet>
     	<servlet-name>UserUpdateServlet</servlet-name>
     	<servlet-class>servlet.UserUpdateServlet</servlet-class>
     </servlet>
     
     <servlet-mapping>
     	<servlet-name>UserUpdateServlet</servlet-name>
     	<url-pattern>/UserUpdateServlet</url-pattern>
     </servlet-mapping>
     
     <servlet>
     	<servlet-name>deleteServlet</servlet-name>
     	<servlet-class>servlet.deleteServlet</servlet-class>
     </servlet>
     
     <servlet-mapping>
     	<servlet-name>deleteServlet</servlet-name>
     	<url-pattern>/deleteServlet</url-pattern>
     </servlet-mapping>
     
     <servlet>
     	<servlet-name>getTotUsersServlet</servlet-name>
     	<servlet-class>servlet.getTotUsersServlet</servlet-class>
     </servlet>
     
     <servlet-mapping>
     	<servlet-name>getTotUsersServlet</servlet-name>
     	<url-pattern>/getTotUsersServlet</url-pattern>
     </servlet-mapping>
     
     <servlet>
     	<servlet-name>AdminLoginServlet</servlet-name>
     	<servlet-class>servlet.AdminLoginServlet</servlet-class>
     </servlet>
     
     <servlet-mapping>
     	<servlet-name>AdminLoginServlet</servlet-name>
     	<url-pattern>/AdminLoginServlet</url-pattern>
     </servlet-mapping>
     
     <servlet>
    <servlet-name>MusicListServlet</servlet-name>
    <servlet-class>servlet.MusicListServlet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>MusicListServlet</servlet-name>
    <url-pattern>/MusicListServlet</url-pattern>
</servlet-mapping>

<servlet>
    <servlet-name>moderatorLoginServlet</servlet-name>
    <servlet-class>servlet.moderatorLoginServlet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>moderatorLoginServlet</servlet-name>
    <url-pattern>/moderatorLoginServlet</url-pattern>
</servlet-mapping>
     
</web-app>
